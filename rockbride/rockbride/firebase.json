{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/photography",
        "destination": "/photography.html"
      },
      {
        "source": "/contact",
        "destination": "/contact.html"
      }
    ],
    "errorPages": [
      {
        "source": "/404",
        "destination": "/404.html"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/*.bat"
    ]
  }
}